header.header
	.container-fluid
		.row
			.col-12
				.header__content
					.header__logo
						a(href="/")
							img(src="assets/img/logo.svg", alt="")

					ul.main-menu
						li.main-menu__item
							a(href="/")
								+svg('ic-home','ic-home', '24', '25')
						-
							var list=[
								{text:'История'},
								{text:'Портфель'},
								{text:'О компании'},
								{text:'Контакты'},
							]
						each item in list
							li.main-menu__item
								a(href="#").main-menu__link= item.text
